    <!-- SweetAlert-->
    <script src="files/sweetalert/sweetalert2.all.min.js"></script>
    <script src="files/sweetalert/sweetalert2.min.js"></script>
    <!-- Login Modal -->
    <script src="files/kingMaterials/pagecssjs/loginModal/js.js"></script>
