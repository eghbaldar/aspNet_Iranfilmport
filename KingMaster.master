    <script type="application/ld+json">
        {
          "@context": "http://schema.org",
          "@type": "Organization",
          "url": "http://www.iranfilmport.com",
          "name": "IRAN FILM PORT - درگاه فیلم ایران",
          "contactPoint": {
            "@type": "ContactPoint",
            "telephone": "+98-921-634-3320",
            "contactType": "Customer service"
          }
        }
