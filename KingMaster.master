<body>
    <form id="form1" runat="server">
        <asp:ScriptManager ID="ScriptManager1" runat="server"></asp:ScriptManager>
        <!-- Login - Modal content -->
        <uc4:KingLoginModal ID="Login1" runat="server" />
        <!-- Top Search -->
        <uc3:KingFlowingControls ID="KingFlowingControls1" runat="server" />
        <!-- Top Search -->
        <uc5:KingTopSearch ID="KingTopSearch1" runat="server" />
        <!-- Advertisement Modal -->
        <uc6:KingAdvertiseModal ID="KingAdvertiseModal1" runat="server" />
        <!-- Preloader Start -->
        <div id="preloader-active">
            <div class="preloader d-flex align-items-center justify-content-center">
                <div class="preloader-inner position-relative">
                    <div class="text-center">
                        <img class="jump mb-50" src="assets/imgs/loading.svg" alt="">
                        <h6>درگاه فیلم ایران ...</h6>
                        <div class="loader">
                            <div class="bar bar1"></div>
                            <div class="bar bar2"></div>
                            <div class="bar bar3"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <aside id="sidebar-wrapper" class="custom-scrollbar offcanvas-sidebar position-right">
        </aside>

        <!-- Main Header -->
        <header class="main-header header-style-2 mb-40 kingHeaderShadow">
            <div class="header-bottom header-sticky background-white text-center ">

                <div class="mobile_menu d-lg-none d-block"></div>
                <div class="container ">
                    <div class="row">
                        <div class="col-lg-2 col-md-3 ">
                            <div class="header-logo d-none d-lg-block">
                                <a href="/">
                                    <img class="logo-img d-inline" src="/files/KingMaterials/imgs/logos/iranfilmport_movibeta_2.png" alt="">
                                </a>
                            </div>
                            <div class="logo-tablet d-md-inline d-lg-none d-none">
                                <a href="/">
                                    <img class="logo-img d-inline" src="assets/imgs/logo.svg" alt="">
                                </a>
                            </div>
                            <div class="logo-mobile d-block d-md-none">
                                <a href="/">
                                    <img class="logo-img d-inline" src="assets/imgs/favicon.svg" alt="">
                                </a>
                            </div>
                        </div>
                        <div class="col-lg-10 col-md-9 main-header-navigation">
                            <!-- Main-menu -->
                            <uc1:KingTopHeader ID="KingTopHeader1" runat="server" />
                            <!-- Search -->

                            <input type="text" onclick="openSearch()" class="search_field" placeholder="جستجو ..." value="" name="s">

                            <a href="http://twitter.com/iranfilmport" target="_blank"><i class="fa-brands fa-x-twitter"></i></a>
                            <a href="http://fb.com/iranfilmport" target="_blank"><i class="fa-brands fa-facebook"></i></a>
                            <a href="http://instagram.com/iranfilmport" target="_blank"><i class="fa-brands fa-instagram"></i></a>
                            <a onclick="$('#loginModal').modal();"><i class="fa fa-lock pointer" id="myBtn" aria-hidden="true"></i></a>
                        </div>
                    </div>
                </div>
                <div class="scroll-progress gradient-bg-1"></div>
            </div>
        </header>

        <!-- Main Wrap Start -->
        <div>
            <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
            </asp:ContentPlaceHolder>
        </div>
        <!-- Footer Start-->
        <!-- footer-bottom aera -->
        <uc2:KingFooter ID="KingFooter1" runat="server" />
        <!-- Footer End-->
        <!-- Main Wrap End-->
        <div class="dark-mark"></div>
    </form>
    <!-- Vendor JS-->
    <script src="/files/kingMaterials/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="/files/kingMaterials/js/vendor/jquery-3.6.0.min.js"></script>
